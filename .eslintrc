{
	"env": {
		"browser": true,
		"es6": true,
		"node": true,
		"commonjs": true
	},
	"extends": [
		"plugin:vue/essential",
		"eslint:recommended"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly",
		"WALLE_BOT_TIMESTAMP": "readonly",
		"WALLE_BOT_MODE": "readonly",
		"toastr": "readonly"
	},
	"parserOptions": {
		"parser": "babel-eslint",
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"no-irregular-whitespace": 0,
		"no-empty": 0,
		"no-extra-semi": 0,
		"no-constant-condition": 0,
		"no-unused-vars": 0,
		"no-console": 0
	}
}